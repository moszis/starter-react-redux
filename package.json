{
  "name": "starter-react-redux",
  "version": "1.0.0",
  "description": "Minimal boilerplate for react with redux starter project",
  "keywords": [
    "react",
    "redux",
    "minimal",
    "starter kit",
    "boilerplate"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/moszis/starter-react-redux"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot --history-api-fallback --host localhost --port 8080",
    "build-dev": "webpack --config config/webpack.dev.config.js",
    "build-prod": "webpack -p --config config/webpack.prod.config.js",
    "test": "mocha --recursive --compilers js:babel-register --require babel-polyfill --require ignore-styles",
    "test-watch": "npm test -- --watch"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-3"
    ]
  },
  "author": "Sergey Pugach",
  "license": "MIT",

  
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "react-router-redux": "^4.0.7",
    "react-bootstrap": "^0.30.7",
    "react-router-bootstrap": "^0.23.1",

    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-saga": "^0.14.3"
  },


  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-runtime": "^6.20.0",

    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",

    "css-loader": "^0.26.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "node-sass": "^4.3.0",

    "clean-webpack-plugin": "^0.1.15", 
    "extract-text-webpack-plugin": "^1.0.1",

    "enzyme": "^2.7.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^3.2.0",

    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "redux-freeze": "^0.1.5",
    "whatwg-fetch": "^2.0.1"
  }
}
